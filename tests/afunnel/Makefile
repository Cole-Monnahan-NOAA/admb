all:
	$(MAKE) simple3
	$(MAKE) run
run:
	time ./simple3
simple3:
	../../admb simple3
clean:
	@rm -vf admodel.cov
	@rm -vf admodel.dep
	@rm -vf admodel.hes
	@rm -vf fmin.log
	@rm -vf simple3
	@rm -vf simple3.bar
	@rm -vf simple3.cor
	@rm -vf simple3.cpp
	@rm -vf simple3.eva
	@rm -vf simple3.htp
	@rm -vf simple3.log
	@rm -vf simple3.obj
	@rm -vf simple3.par
	@rm -vf simple3.std
